{
  "name": "pictophone",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": "https://github.com/hjfreyer/pictophone-be",
  "scripts": {
    "test": "jest",
    "tsc": "tsc",
    "dev": "gcloud config set project pictophone-app && FIRESTORE_EMULATOR_HOST=localhost:8181 ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "test-live": "gcloud config set project pictophone-test && PORT=3111 nodemon ./src/index.ts",
    "test-live-inspect": "gcloud config set project pictophone-test && PORT=3111 nodemon --exec node --inspect -r ts-node/register ./src/index.ts",
    "prod-live": "gcloud config set project pictophone-app && ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "prod": "tsc && node ./dist/index.js",
    "gen": "npm-run-all gen:*",
    "gen:model": "bash genmodel.sh",
    "gen:schema": "cargo run --manifest-path schemac/Cargo.toml -- src/schema/schema.yaml",
    "gen:schema_validator": "typescript-json-validator --collection  src/schema/interfaces.ts ",
    "start": "node ./dist/index.js",
    "firestore": "gcloud config set project pictophone-app && gcloud beta emulators firestore start --host-port localhost:8181",
    "sample-data": "ts-node src/sample_data.ts",
    "inspect": "FIRESTORE_EMULATOR_HOST=localhost:8181 ts-node",
    "fmt": "npm-run-all fmt:*",
    "fmt:ts": "tsfmt -r",
    "fmt:rust": "cargo fmt --manifest-path schemac/Cargo.toml"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/deep-diff": "^1.0.0",
    "@types/deep-equal": "^1.0.1",
    "@types/express": "^4.17.6",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.13",
    "@types/node-fetch": "^2.5.7",
    "@types/request": "^2.48.5",
    "npm-run-all": "^4.1.5",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5",
    "typescript-formatter": "^7.2.2",
    "typescript-json-schema": "^0.42.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "deep-diff": "^1.0.2",
    "deep-equal": "^2.0.3",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "firebase-admin": "^8.12.1",
    "immer": "^7.0.1",
    "ix": "^3.0.2",
    "js-sha256": "^0.9.0",
    "lodash": "^4.17.19",
    "ts-node": "^8.10.2",
    "typescript-json-validator": "^2.4.2"
  }
}
